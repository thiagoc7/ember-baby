<div class="row">
  <div class="col m6 s12 offset-m3">
    <div class="card-panel center-align">
      {{#if timerConfirmed}}
        {{timer-main}}
      {{else}}
        {{timer-before lastStart=lastStart}}
      {{/if}}
    </div>
  </div>
</div>

<div class="row">
  <div class="col m8 s12 offset-m2">
    <div class="card-panel center-align">
      {{timer-table model=model}}
    </div>
  </div>
</div>
