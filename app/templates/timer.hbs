<div class="row">
  <div class="col m6 s12 offset-m3">
    {{#liquid-if timerConfirmed class="timer-components"}}
      {{timer-main start=newTimerStart}}
    {{else}}
      {{timer-before lastStart=lastStart}}
    {{/liquid-if}}
  </div>
</div>

<div class="row">
  <div class="col m8 s12 offset-m2">
    {{timer-table model=model}}
  </div>
</div>
